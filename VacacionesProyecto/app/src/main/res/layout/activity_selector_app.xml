<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".SelectorApp">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:gravity="center"
        android:orientation="vertical">

        <ImageButton
            android:id="@+id/SelectorNotas"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:background="@null"
            android:contentDescription="@string/Descripcion_defecto"
            android:onClick="abrirNotas"
            android:src="@drawable/ic_logo_notas" />

        <ImageButton
            android:id="@+id/SeectorGastos"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:background="@null"
            android:contentDescription="@string/Descripcion_defecto"
            android:src="@drawable/ic_exposure_plus_1_red_50dp" />


    </LinearLayout>


</RelativeLayout>